ns from \"./constans.js\";\nimport * as html2canvas from \"html2canvas\";\nimport * as Actions from \"./actions/index\";\n\nconst App = () => {\n  const [state, dispatch] = useReducer(reducer, initialState);\n  const previewData = useRef(null);\n\n  const savePreview = async () => {\n    let imageurl;\n    await html2canvas(previewData.current, { allowTaint: false }).then(\n      (canvas) => {\n        imageurl = canvas.toDataURL(\"image/jpeg\");\n      }\n    );\n\n    dispatch({ type: Actions.SAVE_PREVIEW, imageurl });\n  };\n\n  return (\n    <AppContext.Provider value={{ state, dispatch }}>\n      <div className=\"App\" ref={previewData}>\n        <DefaultDialog />\n        <Header savePreview={savePreview} />\n        <div className=\"content-wrapper\">\n          <div className=\"upper-wrapper\">\n            <div className=\"content-1\">\n              <Card tag={\"名前\"} item={Constans.NAME} />\n              <Card tag={\"性別\"} item={Constans.SEX} />\n              <Card tag={\"誕生日\"} item={Constans.BIRTHDAY} />\n            </div>\n            <ProfileImg />\n          </div>\n          <LargeCard tag={\"病名\"} item={Constans.DISEASENAME} />\n          <LargeCard tag={\"治療歴\"} item={Constans.TREATMENT_HISTORY} />\n          <LargeCard tag={\"悩み\"} item={Constans.TROUBLE} />\n          <LargeCard tag={\"こんな人と繋がりたい！！\"} item={Constans.CONNECT} />\n        </div>\n      </div>\n    </AppContext.Provider>\n  );\n};\n\nexport default App;\n","/Users/kazuya/workplace/app-development/patient-profile/src/components/index.js",[],"/Users/kazuya/workplace/app-development/patient-profile/src/components/UIkit/DefaultButton.jsx",[],"/Users/kazuya/workplace/app-development/patient-profile/src/components/UIkit/Header.jsx",[],["131","132"],"/Users/kazuya/workplace/app-development/patient-profile/src/components/UIkit/LargeCard.jsx",[],"/Users/kazuya/workplace/app-development/patient-profile/src/components/UIkit/Tag.jsx",[],"/Users/kazuya/workplace/app-development/patient-profile/src/components/UIkit/Card.jsx",["133","134","135","136"],"import React, { useContext } from \"react\";\nimport { Tag, CangedIconButton } from \"../index\";\nimport AppContext from \"../../contexts/AppContext\";\nimport { DIALOG_OPEN } from \"../../actions/index\";\nimport * as Constans from \"../../constans\";\nimport Name from \"../../icons/name.png\";\nimport Sex from \"../../icons/sex.png\";\nimport Birthday from \"../../icons/birthday.png\";\n\nconst Card = ({ tag, item, changeIconBtn = false }) => {\n  const { state, dispatch } = useContext(AppContext);\n\n  const icon = (function () {\n    switch (item) {\n      case Constans.NAME:\n        return <img src={Name} className=\"icon\" />;\n      case Constans.SEX:\n        return <img src={Sex} className=\"icon\" />;\n      case Constans.BIRTHDAY:\n        return <img src={Birthday} className=\"icon\" />;\n      default:\n        return <img src={Name} className=\"icon\" />;\n    }\n  })();\n\n  const content = (function () {\n    switch (item) {\n      case Constans.NAME:\n        return state.name;\n      case Constans.SEX:\n        return state.sex;\n      case Constans.BIRTHDAY:\n        const birthday = state.birthday;\n        if (birthday.moon && birthday.day) {\n          return `${state.birthday.moon}月${state.birthday.day}日`;\n        }\n        return \"\";\n      default:\n        return \"\";\n    }\n  })();\n\n  return (\n    <div className=\"content-1__card\">\n      <div className=\"tag-side\">\n        {icon}\n        <Tag name={tag} icon={false} />\n      </div>\n      <p className=\"content\">\n        {content}\n        <span>\n          <CangedIconButton\n            onClick={() => dispatch({ type: DIALOG_OPEN, item })}\n            className={changeIconBtn ? \"diplay-none\" : false}\n          />\n        </span>\n      </p>\n    </div>\n  );\n};\n\nexport default Card;\n","/Users/kazuya/workplace/app-development/patient-profile/src/components/UIkit/ProfileImg.jsx",[],"/Users/kazuya/workpl